    p_ORGANIZATION_ID IN TK_ORG_ORDER_CONFIG.ORGANIZATION_ID%type,
  outCursor OUT DSS.MYGEN.sqlcur)
IS
BEGIN
    OPEN outCursor FOR
    SELECT
        OPERATING_UNIT,
        ORGANIZATION_ID,
        LINE_SEQ,
        ORDER_TYPE,
        ORDER_DESCRIPTION,
        ORDER_SHORT_CODE,
        ORDER_EXPIRED,
        ORDER_FILTER,
