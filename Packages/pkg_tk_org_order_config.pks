        ATTRIBUTE4,
        ATTRIBUTE5
    FROM TK_ORG_ORDER_CONFIG
    WHERE
        LINE_SEQ = p_LINE_SEQ AND
        ORGANIZATION_ID = p_ORGANIZATION_ID
;
END P_LoadByPrimaryKey;

