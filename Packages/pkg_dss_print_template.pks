      p_SIZE_H          IN DSS_PRINT_TEMPLATE.SIZE_H%TYPE,
      p_SIZE_W          IN DSS_PRINT_TEMPLATE.SIZE_W%TYPE,
      p_UNITS           IN DSS_PRINT_TEMPLATE.UNITS%TYPE,
      p_HAS_IMAGE       IN DSS_PRINT_TEMPLATE.HAS_IMAGE%TYPE,
      p_SAMPLE_IMAGE    IN DSS_PRINT_TEMPLATE.SAMPLE_IMAGE%TYPE,
      p_IS_ACTIVE       IN DSS_PRINT_TEMPLATE.IS_ACTIVE%TYPE,
      p_STATUS          IN DSS_PRINT_TEMPLATE.STATUS%TYPE,
      p_CREATE_BY       IN DSS_PRINT_TEMPLATE.CREATE_BY%TYPE,
